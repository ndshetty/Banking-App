<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="finalproject.CustomerPageController">
   <children>
      <Label layoutX="125.0" layoutY="14.0" text="Welcome Customer!">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="depositbtn" layoutX="23.0" layoutY="57.0" mnemonicParsing="false" onAction="#handleDepositAction" text="Deposit Money" />
      <Button fx:id="withdrawbtn" layoutX="18.0" layoutY="114.0" mnemonicParsing="false" onAction="#handleWithdrawAction" text="Withdraw Money" />
      <Button fx:id="opbtn" layoutX="234.0" layoutY="57.0" mnemonicParsing="false" onAction="#handleOPAction" text="Do Online Purchases" />
      <Button fx:id="logoutbtn" layoutX="309.0" layoutY="248.0" mnemonicParsing="false" onAction="#handleLogoutAction" text="Logout" />
      <Label layoutX="23.0" layoutY="168.0" text="\$" />
      <TextField fx:id="amountfield" layoutX="35.0" layoutY="164.0" promptText="Please enter amount" />
      <Label fx:id="amountlabel" layoutX="21.0" layoutY="197.0" prefHeight="17.0" prefWidth="198.0" />
      <Label fx:id="secondlabel" layoutX="19.0" layoutY="231.0" prefHeight="17.0" prefWidth="180.0" />
   </children>
</AnchorPane>
