<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="finalproject.ManagerController">
   <children>
      <Label layoutX="77.0" layoutY="27.0" text="Welcome Manager!">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="addbtn" layoutX="37.0" layoutY="75.0" mnemonicParsing="false" onAction="#handleAddAction" text="Add Customer" />
      <Button fx:id="logoutbtn" layoutX="216.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleLogoutAction" text="Logout" />
      <Button fx:id="deletebtn" layoutX="160.0" layoutY="75.0" mnemonicParsing="false" onAction="#handleDeleteAction" text="Delete Customer" />
   </children>
</AnchorPane>
